        <externalId>false</externalId>
        <label>Disbursed Via:</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <controllingField>Disbursement_Status__c</controllingField>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Pick-Up</fullName>
                    <default>false</default>
                    <label>Pick-Up</label>
                </value>
                <value>
                    <fullName>Mailed</fullName>
                    <default>false</default>
                    <label>Mailed</label>
                </value>
            </valueSetDefinition>
            <valueSettings>
                <controllingFieldValue>Disbursed</controllingFieldValue>
                <valueName>Pick-Up</valueName>
            </valueSettings>
            <valueSettings>
                <controllingFieldValue>Disbursed</controllingFieldValue>
                <valueName>Mailed</valueName>
            </valueSettings>
        </valueSet>
    </fields>
