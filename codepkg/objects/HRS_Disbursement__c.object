        <displayFormat>HRSD-{000000}</displayFormat>
        <label>HRS Disbursement Name</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>HRS Disbursements</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Check_Date_Required_if_Paid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISNEW()),
   ISPICKVAL( Disbursement_Status__c , &apos;Disbursed&apos;),
   ISPICKVAL( Disbursement_Status__c , &apos;Paid&apos;),   
   ISBLANK( Check_Date__c )
)</errorConditionFormula>
        <errorMessage>Check date is required when the disbursement is cut</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_number_Required_if_Paid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISNEW()),
ISPICKVAL( Disbursement_Status__c , &apos;Disbursed&apos;),
ISPICKVAL( Disbursement_Status__c , &apos;Paid&apos;),
ISBLANK( Check_Number__c )
)</errorConditionFormula>
        <errorMessage>Check number is required when the disbursement is cut</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disbursed_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISNEW()),
ISPICKVAL(  Disbursed_Via__c  ,&apos;Pick-Up&apos;),
ISPICKVAL(  Disbursed_Via__c  ,&apos;Mailed&apos;),
ISBLANK( Disbursed_Date__c )
)</errorConditionFormula>
        <errorMessage>Disbursed Date is required when the payment has been disbursed</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
