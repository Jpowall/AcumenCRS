    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Individual line items of escrow disbursements for each loan</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>Check_Date__c</fullName>
        <externalId>false</externalId>
        <label>Check Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Check_Number__c</fullName>
        <externalId>false</externalId>
        <label>Check Number</label>
        <length>18</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Check_Transferred_by__c</fullName>
        <externalId>false</externalId>
        <label>Disbursed Via:</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <controllingField>Disbursement_Status__c</controllingField>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Pick-Up</fullName>
                    <default>false</default>
                    <label>Pick-Up</label>
                </value>
                <value>
                    <fullName>Mailed</fullName>
                    <default>false</default>
                    <label>Mailed</label>
                </value>
            </valueSetDefinition>
            <valueSettings>
                <controllingFieldValue>Disbursed</controllingFieldValue>
                <valueName>Pick-Up</valueName>
            </valueSettings>
            <valueSettings>
                <controllingFieldValue>Disbursed</controllingFieldValue>
                <valueName>Mailed</valueName>
            </valueSettings>
        </valueSet>
    </fields>
    <fields>
        <fullName>Contractor__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>Enter the contractor which the check is issued to</inlineHelpText>
        <label>Contractor</label>
        <referenceTo>Contractors__c</referenceTo>
        <relationshipName>Disbursements</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Disbursed_Date__c</fullName>
        <externalId>false</externalId>
        <label>Disbursed Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Disbursement_Status__c</fullName>
        <description>To better track status of disbursements, and be able to input them prior to being paid.</description>
        <externalId>false</externalId>
        <label>Disbursement Status</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Draft</fullName>
                    <default>false</default>
                    <label>Draft</label>
                </value>
                <value>
                    <fullName>Submitted</fullName>
                    <default>false</default>
                    <label>Submitted</label>
                </value>
                <value>
                    <fullName>Paid</fullName>
                    <default>false</default>
                    <label>Paid</label>
                </value>
                <value>
                    <fullName>Disbursed</fullName>
                    <default>false</default>
                    <label>Disbursed</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Draw_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>Invoice_Amount__c  -  Retainer_Amount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Draw Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Draw_Description__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>In a few words, describe the reason for the draw</inlineHelpText>
        <label>Draw Description</label>
        <length>100</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Final_Bill__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This field controls whether a retainer is held or not. Should only ever be checked once per project, as it denotes when all the monies should be emptied from the loan</description>
        <externalId>false</externalId>
        <inlineHelpText>Check whether this is the final bill for the project. No retainer will be held if you check this box.</inlineHelpText>
        <label>Final Bill?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Invoice_Amount__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter the amount of the invoice</inlineHelpText>
        <label>Invoice Amount</label>
        <precision>12</precision>
        <required>true</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Labor_Costs__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter the labor costs, if any, on the invoice</inlineHelpText>
        <label>Labor Costs</label>
        <precision>12</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Labor__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Enter whether or not the draw is for labor costs</inlineHelpText>
        <label>Labor?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Loan_Number__c</fullName>
        <externalId>false</externalId>
        <label>Loan#</label>
        <referenceTo>Loan__c</referenceTo>
        <relationshipName>Disbursements</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>true</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Materials__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Check whether or not the draw is for materials</inlineHelpText>
        <label>Materials?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Project_number__c</fullName>
        <externalId>false</externalId>
        <label>Project#</label>
        <referenceTo>Projects__c</referenceTo>
        <relationshipName>Disbursements</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>true</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Property_Owner__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>Enter the name of the property owner</inlineHelpText>
        <label>Property Owner</label>
        <referenceTo>Contact</referenceTo>
        <relationshipName>Disbursements</relationshipName>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Release_Retainer__c</fullName>
        <defaultValue>false</defaultValue>
        <description>For paying out retainers back  to contractors</description>
        <externalId>false</externalId>
        <inlineHelpText>Check if the disbursement is releasing the retainer back to the contractor</inlineHelpText>
        <label>Release Retainer?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Retainer_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Final_Bill__c , 0, ((Labor_Costs__c)  * 0.10))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Retainer Held</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Retainer_Draw_Amount__c</fullName>
        <description>This field is used to track how much retainer is left to be paid with roll-up summaries on project and loan records</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter in the retainer amount that is released</inlineHelpText>
        <label>Retainer Draw Amount</label>
        <precision>12</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <label>Loan Disbursement</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>ED-{000000000}</displayFormat>
        <label>Disbursement #</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Loan Disbursements</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Check_Date_Required_if_Paid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISNEW()),
   ISPICKVAL( Disbursement_Status__c , &apos;Disbursed&apos;),
   ISPICKVAL( Disbursement_Status__c , &apos;Paid&apos;),   
   ISBLANK( Check_Date__c)
)</errorConditionFormula>
        <errorMessage>Check date is required when the disbursement is cut</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_number_Required_if_Paid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISNEW()),
   ISPICKVAL( Disbursement_Status__c , &apos;Disbursed&apos;),
   ISPICKVAL( Disbursement_Status__c , &apos;Paid&apos;),
   ISBLANK(  Check_Number__c )
)</errorConditionFormula>
        <errorMessage>Check number is required when the disbursement is cut</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disbursed_Date_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISNEW()),
   ISPICKVAL( Check_Transferred_by__c ,&apos;Pick-Up&apos;),
   ISPICKVAL( Check_Transferred_by__c ,&apos;Mailed&apos;),
   ISBLANK( Disbursed_Date__c )
)</errorConditionFormula>
        <errorMessage>Disbursed Date is required when the payment has been disbursed</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
